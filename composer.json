{
    "name": "kabuto/dependent-select-bundle",
    "type": "symfony-bundle",
    "description": "This bundle helps to create dependent AJAX fields for Symfony forms",
    "keywords": ["ajax dependent form", "dependent select"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Belous",
            "homepage": "http://github.com/serhiobb",
            "role": "Developer"
        },
        {
            "name": "Evercode Lab",
            "homepage": "http://evercodelab.com",
            "role": "Developer"
        },
        {
            "name": "Artemy Antcev",
            "homepage": "http://github.com/perkelekurat",
            "role": "Developer"
        },
        {
            "name": "Shtumi",
            "homepage": "http://github.com/shtumi",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "symfony/framework-bundle": "^2.8 || ^3.0 || ^4.0"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "kabuto/dependent-select-bundle",
                "version": "dev-master",
                "source": {
                    "url": "git://github.com/serhiobb/DependentSelectBundle.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "psr-4" : {
                        "Evercode\\DependentSelectBundle\\": ""
                    }
                }
            }
        }
    ],
    "suggest": {
        "sonata-project/admin-bundle": "master-dev",
        "sonata-project/doctrine-orm-admin-bundle": "master-dev"
    },
    "autoload": {
        "psr-4": { "Evercode\\DependentSelectBundle\\": "" }
    }
}
